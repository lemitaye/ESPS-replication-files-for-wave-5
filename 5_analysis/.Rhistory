mean_tbl <- function(tbl, vars = vars_both, group_vars, pw) {
tbl %>%
pivot_longer(all_of(vars),
names_to = "variable",
values_to = "value") %>%
group_by(pick(group_vars)) %>%
summarise(
mean = weighted.mean(value, w = {{pw}}, na.rm = TRUE),
nobs = sum(!is.na(value)),
.groups = "drop"
)
}
